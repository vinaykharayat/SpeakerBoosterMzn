{

    "manifest_version": 2,
    "name": "Speaker Booster",
    "version": "1.0.1",

    "description": "Boosts up the speaker volume",

    "icons": {
        "64": "icons/icon.png"
    },

    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content_script.js"]
    }],
    "browser_action": {
        "default_icon": {
            "64": "icons/icon.png"
        },
        "default_popup": "browserAction/index.html",
        "default_title": "Speaker Booster"
    },

    "permissions": [
        "storage"
    ]
}